{
	"name": "compono-kit/money-formatters",
	"description": "Money formatter classes for implementations of compono-kit/money-interfaces",
	"homepage": "https://github.com/compono-kit/money-formatters",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"license": "proprietary",
	"authors": [
		{
			"name": "Benjamin Bruska",
			"email": "benjamin.bruska@your-coder.de"
		}
	],
	"require": {
		"php": ">=8.3",
		"ext-intl": "*",
		"compono-kit/money-interfaces": "^1.0.0"
	},
	"require-dev": {
		"roave/security-advisories": "dev-latest",
		"phpunit/phpunit": "^12.0.0"
	},
	"autoload": {
		"psr-4": {
			"ComponoKit\\Money\\Formatters\\": "src/"
